<!DOCTYPE html>
<html>
<head>
    <title>Students</title>
</head>
<body>
    <h1>Students</h1>
    <table border="1">
        <tr>
            <th>NAME</th>
            <th>AGE</th>
        </tr>
        <% students.forEach(function(student) { %>

        <tr>
                <td><%= student.name %> </td><td><%= student.age %></td>
        </tr>
        <% }); %>

    </table>
    
    <a href="/logout">Logout</a>
</body>
</html>
